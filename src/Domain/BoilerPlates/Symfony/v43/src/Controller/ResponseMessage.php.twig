{% if action == 'index' %}
        return $this->render('{{ name }}/index.html.twig', [
            'registers' => $response->{{ item }},
        ]);
{% elseif action in ['create', 'update'] %}
        $this->addFlash($response->status, $response->message);

        return $this->redirectToRoute('{{ entity }}.index');
{% elseif action == 'read' %}
        return $this->render('{{ name }}/show.html.twig', [
            'register' => $response->{{ name }},
        ]);
{% else %}
        return new Response($response);
{% endif %}